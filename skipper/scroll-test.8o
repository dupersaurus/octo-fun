# Compatibility: XO-Chip
# 100 cycles/frame
#
# BG: #000
# F1: #fff
# F2: #609
# Bl: #fff

:alias x v1
:alias y v2
:alias height v3
:alias slice-index v4
:alias background-step v5
:alias background-length v6

:alias py-old	va
:alias py		vb
:alias vy		vc
:alias vy-step	vd
:alias dv		ve

:const BACKGROUND_X 	124
:const BACKGROUND_TOP_Y 10
:const BACKGROUND_BOT_Y 40
:const WATER_SURFACE_Y 	44
:const STONE_X 			64

:const GO_DOWN 0
:const GO_UP 1

:macro draw-stone x y {
	i := long rock
	plane 1
	sprite x y 2
}

:macro draw-mirror-stone x y {
	i := long rock
	plane 1
	sprite x y 2
	plane 2
	sprite x y 2
}

:macro calc-mirror-y y out {
	out := WATER_SURFACE_Y
	out -= y
	out += WATER_SURFACE_Y
}

: main
	hires
	
  scroll-slice-test
;

: scroll-slice-test
	vy := 0
	vy-step := 0
	dv := GO_DOWN

	x := 0
	v0 := 15
	slice-index := 0
	plane 1

	va := 0
	py := 26
	dv := GO_DOWN
	i := long rock
	draw-stone va py
	
	loop
		plane 2
		i := long water-surface
		
		y := BACKGROUND_TOP_Y
		sprite x y 15

		i += v0
		y += 15
		sprite x y 15

		i := long rock
		draw-stone va py
		va += 2
		draw-stone va py

		x += 4
		slice-index += 1
		if slice-index != 32 then
	again
	
	slice-index := 0
	background-step := 0
	start-landscape

	py-old := py

	loop
		x := STONE_X
		calc-mirror-y py-old y
		draw-stone x py-old
		draw-mirror-stone x y
		plane 3
		scroll-left
	
		draw-next-slice
		step-stone
	
		vf := 10
		delay := vf
		
		loop
			vf := delay
			if vf != 0 then
		again
	again
;

: step-stone
	py-old := py

	x := STONE_X
	draw-stone x py

	x := STONE_X
	calc-mirror-y py y
	draw-mirror-stone x y

	update-vertical-speed
	move-stone
;

: update-vertical-speed
	if vy == 255 then jump move-stone

	if dv == GO_DOWN 	then jump update-dv-down
	if dv == GO_UP 		then jump update-dv-up
;

: update-dv-up
	v0 := 10
	vy -= v0
	
	if vf == 0 then dv := GO_DOWN
;

: update-dv-down
	v0 := 10
	vy += v0
	
	if vf == 1 then vy := 255
;

: move-stone
	vy-step += vy
	if vf == 1 begin
		if dv == GO_UP then py += -1
		if dv == GO_DOWN then py += 1
	end

	if py == WATER_SURFACE_Y then splish-splash
;

: splish-splash
	dv := GO_UP
	neutral-spash
;

: neutral-spash
	v0 := 30
	vy -= v0
;

: draw-next-slice
	#:breakpoint argh
	v0 := background-step
	jump0 landscape-steps
;

: draw-slice
	x := BACKGROUND_X
	y := BACKGROUND_TOP_Y
	height := 15
	
	plane 2
	sprite x y 15
	y += height
	i += height
	sprite x y 15
	
	draw-next-mirror-slice
;

: draw-next-mirror-slice
	v0 := background-step
	jump0 landscape-mirror-steps
;

: draw-mirror-slice
	x := BACKGROUND_X
	y := BACKGROUND_BOT_Y
	height := 15
	
	plane 2
	sprite x y 15
	y += height
	i += height
	sprite x y 15

	finish-draw-slice
;

: finish-draw-slice
	slice-index += 6
	if slice-index == background-length then next-background-step
;

: next-background-step
	#:breakpoint nbs
	slice-index := 0
	background-step += 4
	
	if background-step == 16 then background-step := 0
	
	start-landscape
;

: start-landscape
	v0 := background-step
	jump0 setup-landscape
;

:const SMALL_WATER_LENGTH 24
:const MEDIUM_WATER_LENGTH 48
:const LONG_WATER_LENGTH 72
:const MOUNTAIN_LENGTH 192
:const BUTT_ISLAND_LENGTH 96

: setup-landscape
	background-length := MOUNTAIN_LENGTH 		return #return return
	background-length := SMALL_WATER_LENGTH 	return #return return
	background-length := BUTT_ISLAND_LENGTH 	return #return return
	background-length := MEDIUM_WATER_LENGTH 		return #return return
;

: landscape-steps
	v0 := slice-index jump0 mountains-jump 				#return return
	v0 := slice-index jump0 small-water-jump 			#return return
	v0 := slice-index jump0 butt-island-jump 			#return return
	v0 := slice-index jump0 medium-water-jump 			#return return
;

: landscape-mirror-steps
	v0 := slice-index jump0 mountain-mirror-jump 		#return return
	finish-draw-slice return 							#return return
	v0 := slice-index jump0 butt-island-mirror-jump 	#return return
	finish-draw-slice return 							#return return
;

