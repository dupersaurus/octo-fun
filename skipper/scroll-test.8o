:alias x v1
:alias y v2
:alias height v3
:alias slice-index v4
:alias background-step v5
:alias background-length v6

:const BACKGROUND_X 124
:const BACKGROUND_TOP_Y 10
:const BACKGROUND_BOT_Y 40

: main
	hires
	
  scroll-slice-test
;

: scroll-slice-test
	x := 0
	v0 := 15
	slice-index := 0
	plane 1
	
	loop
		i := water-surface
		
		y := BACKGROUND_TOP_Y
		sprite x y 15

		i += v0
		y += 15
		sprite x y 15

		x += 4
		slice-index += 1
		if slice-index != 32 then
	again
	
	x := 64
	y := 32
	i := rock
	sprite x y 2

	slice-index := 0
	background-step := 0
	start-landscape

	loop
		plane 3
		scroll-left
	
		draw-next-slice
	
		vf := 10
		delay := vf
		
		loop
			vf := delay
			if vf != 0 then
		again
	again
;

: pick-background-slice
	slice-index := 0
	background-step := 0
;

: draw-next-slice
	#:breakpoint argh
	v0 := background-step
	jump0 landscape-steps
;

: draw-slice
	x := BACKGROUND_X
	y := BACKGROUND_TOP_Y
	height := 15
	
	plane 1
	sprite x y 15
	y += height
	i += height
	sprite x y 15
	
	draw-next-mirror-slice
;

: draw-next-mirror-slice
	v0 := background-step
	jump0 landscape-mirror-steps
;

: draw-mirror-slice
	x := BACKGROUND_X
	y := BACKGROUND_BOT_Y
	height := 15
	
	plane 2
	sprite x y 15
	y += height
	i += height
	sprite x y 15

	finish-draw-slice
;

: finish-draw-slice
	slice-index += 4
	if slice-index == background-length then next-background-step
;

: next-background-step
	#:breakpoint nbs
	slice-index := 0
	background-step += 8
	
	if background-step == 32 then background-step := 0
	
	start-landscape
;

: start-landscape
	v0 := background-step
	jump0 setup-landscape
;

:const SMALL_WATER_LENGTH 16
:const MEDIUM_WATER_LENGTH 32
:const LONG_WATER_LENGTH 48
:const MOUNTAIN_LENGTH 128
:const BUTT_ISLAND_LENGTH 64

: setup-landscape
	background-length := MOUNTAIN_LENGTH 		return return return
	background-length := SMALL_WATER_LENGTH 	return return return
	background-length := BUTT_ISLAND_LENGTH 	return return return
	background-length := LONG_WATER_LENGTH 		return return return
;

: landscape-steps
	v0 := slice-index jump0 mountains-jump 					return return
	v0 := slice-index jump0 small-water-jump 				return return
	v0 := slice-index jump0 butt-island-jump 				return return
	v0 := slice-index jump0 medium-water-jump 				return return
;

: landscape-mirror-steps
	v0 := slice-index jump0 mountain-mirror-jump 		return return
	finish-draw-slice return return return
	v0 := slice-index jump0 butt-island-mirror-jump 		return return
	finish-draw-slice return return return
;

