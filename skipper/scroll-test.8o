# Compatibility: XO-Chip
# 100 cycles/frame
#
# BG: #000
# F1: #fff
# F2: #609
# Bl: #fff

## situational or temporary ###########
:alias x v1
:alias y v2
:alias height v3

# permanent  ##########################
:alias skip-count		 v4 # total number of skips 

							# TODO save in memory to be made temporary?
:alias slice-index 		 v6	# which slice of a landscape we're drawing
:alias background-step 	 v7 # which landscape we're drawing
:alias background-length v8 # size of the current landscape

:alias next-key-time v5
:alias expected-key v9
:alias actual-key 	va
:alias py-old		vb # TODO could look backwards in arc instead of saving
:alias py			vc
:alias vy			vd # which stone arc we're using
:alias vy-step		ve # position within arc 

:const BACKGROUND_X 	124
:const BACKGROUND_TOP_Y 10
:const BACKGROUND_BOT_Y 40
:const WATER_SURFACE_Y 	44
:const STONE_X 			64
:const KEY_X			60
:const KEY_X_SHIFTED	60
:const KEY_Y			4
:const NULL_KEY			255
:const ABORT_KEY		128
:const ARC_TOTAL_STEPS	8
:const ARC_STEP			6
:calc ARC_MAX { ARC_TOTAL_STEPS * ARC_STEP }
:calc ARC_START { ARC_STEP * 4 }
:calc ARC_END { ARC_MAX - ARC_STEP }

:macro draw-stone x y {
	i := long rock
	plane 1
	sprite x y 2
}

:macro draw-mirror-stone x y {
	i := long rock
	plane 1
	sprite x y 2
}

:macro calc-mirror-y y out {
	out := WATER_SURFACE_Y
	out -= y
	out += WATER_SURFACE_Y
	out -= 1
}

: main
	hires
	scroll-slice-test
;

: scroll-slice-test
	vy := 0
	vy-step := 0

	x := 0
	v0 := 15
	slice-index := 0
	
	## draw the starting water ###############################
	loop
		plane 2
		i := long water-surface
		
		y := BACKGROUND_TOP_Y
		sprite x y 15

		i += v0
		y += 15
		sprite x y 15

		x += 4
		slice-index += 1
		if slice-index != 32 then
	again

	## throw the rock ########################################
	i := arc-start
	v2 := WATER_SURFACE_Y
	v3 := 0
	
	loop
		#:breakpoint loop-step
		i += v3
		load v1

		v2 := WATER_SURFACE_Y
		v2 -= v1
		draw-stone v0 v2

		calc-mirror-y v2 v4
		draw-mirror-stone v0 v4

		wait-timer
		draw-stone v0 v2
		draw-mirror-stone v0 v4

		v3 += 2
		if v1 != 0 then
	again
	
	## play the game ########################################
	py := v2
	vy := ARC_START
	#i := arc-5
	vy-step := 2
	next-key-time := 0x5

	skip-count := 0
	slice-index := 0
	background-step := 0
	expected-key := NULL_KEY
	actual-key := NULL_KEY
	
	start-landscape
	
	py-old := 255
	#:breakpoint start-game

	loop
		if py-old != 255 begin
			x := STONE_X
			calc-mirror-y py-old y
			draw-stone x py-old
			draw-mirror-stone x y
		end

		draw-key

		plane 3
		scroll-left
	
		draw-key
		draw-next-slice
		step-stone
		#:breakpoint after-step

		vf := 10
		delay := vf
		
		loop
			check-input
			vf := delay
			if vf != 0 then
		again

		if expected-key != ABORT_KEY then
	again

	## game over ########################################
	x := STONE_X
	y := WATER_SURFACE_Y

	x += -4
	y += -4

	i := splash-0
	sprite x y 8
	wait-timer

	i := splash-1
	sprite x y 8
	wait-timer

	i := splash-2
	sprite x y 8
	wait-timer

	i := splash-3
	sprite x y 8
	wait-timer

	i := splash-4
	sprite x y 8
	wait-timer

	i := splash-5
	sprite x y 8
	wait-timer

	## start again ######################################
	v0 := key
	plane 3
	clear
	jump scroll-slice-test
;

: wait-timer
	vf := 10
	delay := vf
	
	loop
		vf := delay
		if vf != 0 then
	again
;

: check-input
	if expected-key == NULL_KEY then return

	v0 := OCTO_KEY_A
	if v0 key then jump process-input
	
	v0 := OCTO_KEY_S
	if v0 key then jump process-input
	
	v0 := OCTO_KEY_D
	if v0 key then jump process-input
	
	v0 := OCTO_KEY_F
	if v0 key then jump process-input
;

# v0 is the key
: process-input
	if v0 == expected-key then actual-key := v0
	if v0 != expected-key then actual-key := NULL_KEY
	expected-key := NULL_KEY
;

: key-list 0x7 0x8 0x9 0xE

: select-next-key
	if expected-key != NULL_KEY then return

	draw-key

	v0 := random 0b11
	i := key-list
	i += v0
	load expected-key - expected-key

	draw-key
;

: clear-expected-key
	draw-key
	expected-key := NULL_KEY
;

: draw-key
	if expected-key == NULL_KEY then return

	x := KEY_X
	y := KEY_Y
	select-note-sprite
	
	plane 2
	sprite x y 8
;

: step-stone
	#:breakpoint step-stone
	py-old := py

	x := STONE_X
	draw-stone x py

	x := STONE_X
	calc-mirror-y py y
	draw-mirror-stone x y

	load-skip-arc
;

: move-stone
	#:breakpoint move-stone
	if vy-step == 0 then set-next-key-time

	i += vy-step
	load v0

	if v0 == 0 then jump splish-splash

	py := WATER_SURFACE_Y
	py -= v0
	vy-step += 1

	if expected-key != NULL_KEY then return
	if vy-step == next-key-time then select-next-key
;

: set-next-key-time
	load v0
	next-key-time := v0
;

: splish-splash
	vy-step := 0
	
	if actual-key != NULL_KEY then good-splash
	if actual-key == NULL_KEY then bad-splash

	if expected-key != ABORT_KEY then clear-expected-key
;

: neutral-spash
	vy += 0
;

: bad-splash
	v0 := 5
	buzzer := v0

	v0 := ARC_STEP
	vy -= v0

	if vf == 0 then jump game-over
;

: good-splash
	skip-count += 1
	v0 := ARC_STEP
	vy += v0

	v0 := 20
	buzzer := v0

	if vy == ARC_MAX then vy := ARC_END
;

: game-over
	vy := 0
	expected-key := ABORT_KEY
;

## Stone travel arcs ########################################################
# numbers are y offsets subtracted from WATER_SURFACE_Y
: load-skip-arc
	v0 := vy

	#:breakpoint get-arc
	jump0 arc-jump-table
;

# increment vy by 6
: arc-jump-table
	i := long arc-1		jump move-stone
	i := long arc-2		jump move-stone
	i := long arc-3		jump move-stone
	i := long arc-4		jump move-stone
	i := long arc-5		jump move-stone
	i := long arc-6		jump move-stone
	i := long arc-7		jump move-stone
	i := long arc-8		jump move-stone
;

## Drawing the Landscape ####################################################
: draw-next-slice
	v0 := background-step
	jump0 landscape-steps
;

: draw-slice
	x := BACKGROUND_X
	y := BACKGROUND_TOP_Y
	height := 15
	
	plane 2
	sprite x y 15
	y += height
	i += height
	sprite x y 15
	
	draw-next-mirror-slice
;

: draw-next-mirror-slice
	v0 := background-step
	jump0 landscape-mirror-steps
;

: draw-mirror-slice
	x := BACKGROUND_X
	y := BACKGROUND_BOT_Y
	height := 15
	
	plane 2
	sprite x y 15
	y += height
	i += height
	sprite x y 15

	finish-draw-slice
;

: finish-draw-slice
	slice-index += 6
	if slice-index == background-length then next-background-step
;

: next-background-step
	#:breakpoint nbs
	slice-index := 0
	background-step += 4
	
	if background-step == 16 then background-step := 0
	
	start-landscape
;

: start-landscape
	v0 := background-step
	jump0 setup-landscape
;

:const SMALL_WATER_LENGTH 24
:const MEDIUM_WATER_LENGTH 48
:const LONG_WATER_LENGTH 72
:const MOUNTAIN_LENGTH 192
:const BUTT_ISLAND_LENGTH 96

: setup-landscape
	background-length := MOUNTAIN_LENGTH 		return #return return
	background-length := SMALL_WATER_LENGTH 	return #return return
	background-length := BUTT_ISLAND_LENGTH 	return #return return
	background-length := MEDIUM_WATER_LENGTH 		return #return return
;

: landscape-steps
	v0 := slice-index jump0 mountains-jump 				#return return
	v0 := slice-index jump0 small-water-jump 			#return return
	v0 := slice-index jump0 butt-island-jump 			#return return
	v0 := slice-index jump0 medium-water-jump 			#return return
;

: landscape-mirror-steps
	v0 := slice-index jump0 mountain-mirror-jump 		#return return
	finish-draw-slice return 							#return return
	v0 := slice-index jump0 butt-island-mirror-jump 	#return return
	finish-draw-slice return 							#return return
;

